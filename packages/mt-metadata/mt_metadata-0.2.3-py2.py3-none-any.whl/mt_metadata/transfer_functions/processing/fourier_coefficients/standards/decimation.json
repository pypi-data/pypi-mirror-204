        "description": "Decimation level, must be a positive integer starting at 0.",
        "options": [],
        "alias": [],
        "example": "1",
		"default": null
    },
	"channels": {
        "type": "string",
        "required": true,
        "style": "name list",
        "units": null,
        "description": "list of channels",
