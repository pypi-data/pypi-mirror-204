        "alias": [],
        "example": "default",
		"default": null
    },
	"pre_fft_detrend_type": {
        "type": "string",
        "required": true,
        "style": "free form",
        "units": null,
        "description": "Type of detrend method before FFT.",
        "options": [],
        "alias": [],
        "example": "linear",
		"default": "linear"
    },
	"pre_fft_detrend_type_extra": {
        "type": "string",
        "required": true,
        "style": "free form",
        "units": null,
