<small>
	<svg>
		<use xlink:href="/media/icons.svg#calendar" />
	</svg>
	{{ commits[0].human_time }}
	&nbsp;
	<svg>
		<use xlink:href="/media/icons.svg#clock" />
	</svg>
	{%- if read_time_minutes > 1 %}
	{{ read_time_minutes }} minutes read
	{%- else %}
	{{ read_time_minutes }} minute read
	{%- endif %}
</small>